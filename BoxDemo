class Box
{
double width,height,depth;
Box()
{
System.out.println("Constructing Box");
width=depth=height=10;
}
double volume()
{
return width*height*depth;
}
}
class BoxDemo
{
public static void main(String[] args)
{

double vol;
Box b1=new Box();
vol=b1.volume();
System.out.println("Volume is "+vol);
Box b2=new Box();
vol=b2.volume();
System.out.println("Volume is "+vol);
}
}
